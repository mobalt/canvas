{
  "name": "Canvas Utility Belt",
  "version": "1.0",
  "description": "Adds some functionality to the Canvas site",
  "icons": {
    "64": "icons8-bug-64.png"
  },
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.instructure.com/courses/*/quizzes/*/moderate"
      ],
      "js": [
        "jquery.js",
        "moderate_quiz.js"
      ]
    },
    {
      "matches": [
        "*://*.instructure.com/courses/*/users"
      ],
      "js": [
        "jquery.js",
        "downloadCSV.js",
        "exportStudents.js"
      ]
    },
    {
      "matches": [
        "*://*.instructure.com/courses/*/quizzes/*"
      ],
      "js": [
        "jquery.js",
        "canvasApi.js",
        "quizAssign.js"
      ]
    },
    {
      "matches": [
        "*://*.instructure.com/courses/*/quizzes/*/edit"
      ],
      "js": [
        "jquery.js",
        "js-yaml.js",
        "quizzes.js",
        "quizPost.js"
      ]
    }
  ],
  "manifest_version": 2
}